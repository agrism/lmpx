{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2.5",
    "symfony/config": "^5.3",
    "symfony/console": "5.2.*",
    "symfony/dotenv": "5.2.*",
    "symfony/dependency-injection": "^5.3",
    "symfony/flex": "^1.3.1",
    "monolog/monolog": "^2.3",
    "symfony/monolog-bundle": "^3.1",
    "symfony/property-access": "^5.3",
    "symfony/yaml": "^5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/phpunit-bridge": "^5.3"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  }
}
